generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
}

model Project {
  id            Int    @id @default(autoincrement())
  name          String
  description   String
  image_url     String
  github_url    String
}